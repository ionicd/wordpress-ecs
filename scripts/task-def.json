{
  "family": "wordpress-ecs-app",
  "networkMode": "<default>",
  "containerDefinitions": [
    {
      "name": "wordpress-app",
      "image": "docker.io/sauravkc14/wordpress",
      "portMappings": [
        {
          "containerPort": 9000,
          "hostPort": 9000,
          "protocol": "tcp"
        }
      ],
      "essential": true
    }
  ],
  "requiresCompatibilities": [
    "EC2"
  ],
  "cpu": "1024",
  "memory": "512"
}
